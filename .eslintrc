{
	"env": {
		"browser": true,
		"es2021": true,
		"node": true
	},
	"extends": [
		"airbnb",
		"eslint:recommended",
		"plugin:react/recommended",
		"plugin:react/jsx-runtime"
	],
	"plugins": [
		"react",
		"import",
		"jsx-a11y",
		"react",
		"react-hooks",
		"prettier"
	],
	"rules": {
		"react/jsx-filename-extension": [
			"error",
			{
				"extensions": [".js", ".jsx"]
			}
		],
		"no-console": 0,
		"import/no-unresolved": "off"
	}
}
